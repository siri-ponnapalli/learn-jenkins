pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                script{
                    echo "This is build"
                }
                
            }
        }
        stage('Test') {
            steps {
                script{
                    echo "This is Test"
                }
                
            }
        }
        stage('Deploy') {
            steps {
                script{
                    echo "This is deploy"
                }
                
            }
        }
    }
}